<h1>Songs#new || #edit</h1>
<p>Find me in app/views/songs/new.html.erb</p>

<%= form_for @song do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :artist_id %>
  <%= f.text_field :artist_id %>
  <%= f.label :genre_id %>
  <%= f.text_field :genre_id %>
  <%= f.submit %>
<% end %>
